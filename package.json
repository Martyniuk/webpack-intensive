        "app-root-path": "2.1.0",
        "babel-plugin-dynamic-import-node": "2.2.0",
        "cross-env": "5.2.0",
        "html-webpack-plugin": "3.2.0",
        "moment": "2.22.2",
        "nodegit": "0.23.0",
        "serve": "10.1.1",
        "webpack": "4.27.1",
        "webpack-cli": "3.1.2"
    }
